  // Instance fields
        private string _name;
        private int _price;

        // Constructor
        public Aktie(string name, int price)
        {
            _name = name;
            _price = price;
        }

        // Properties
        public string Name
        {
            get { return _name; }
        }

        public int Price
        {
            get { return _price; }
            set { _price = value; }
        }
        // Metode
